@model IEnumerable<Test_Manager_Alpha.Models.Project>

@{
    ViewBag.Title = "Test Manager: All Projects";
    Layout = "/Views/Shared/_Layout.cshtml";
}

<p><a asp-action="Create">Add project</a></p>
<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td><a asp-action="OpenProject" asp-route-projectId="@item.Id">@item.Name</a></td>
        </tr>
    }
</table>

@section Footer {
    <div>
        <p> </p>
        <p>Copyright© Maxim Gushchin, @DateTime.Now.Year. All rights reserved </p>
    </div>
}